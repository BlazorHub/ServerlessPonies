{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "ServerlessPonies",
      "storageProvider": {
        "connectionStringName": "DurableFunctionsStorageAccount",
        "maxQueuePollingInterval": "00:00:00.100",
        "partitionCount": 4
      },
      "extendedSessionsEnabled": true,
      "extendedSessionIdleTimeoutInSeconds": 30,
      "maxConcurrentActivityFunctions": 250,
      "maxConcurrentOrchestratorFunctions": 250
    }
  }
}